{
  "name": "dental-ecommerce",
  "version": "1.0.0",
  "description": "Plateforme e-commerce pour Ã©quipements dentaires",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start:backend": "cd backend && npm run start:dev",
    "start:frontend": "cd frontend && PORT=3001 npm start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "cd backend && npm test && cd ../frontend && npm test",
    "init:configs": "cd backend && npm run init:configs",
    "db:migrate": "cd backend && npm run migration:run",
    "db:seed": "cd backend && npm run seed"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-repo/dental-ecommerce.git"
  },
  "keywords": [
    "ecommerce",
    "dental",
    "nestjs",
    "react",
    "typescript"
  ],
  "author": "Votre Nom",
  "license": "MIT"
} 